w50Min  <- w50Min
LB <- c(w100Max,w100Min,W50Max,w50Min, w15Min)
Gradation <- c('w100Max','w100Min','W50Max','w50Min','w15Min')
gradationTbl <- data.table(Gradation, LB)
gradationTbl[,Diameter_FT:=dPercent(LB, gammaS)]
return(gradationTbl)
}
